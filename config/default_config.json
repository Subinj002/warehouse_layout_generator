{
    "warehouse": {
      "name": "Default Warehouse",
      "dimensions": {
        "width": 100.0,
        "length": 150.0,
        "height": 12.0,
        "units": "meters"
      },
      "grid_size": 0.5
    },
    "elements": {
      "racks": {
        "types": {
          "standard_pallet": {
            "width": 2.7,
            "depth": 1.1, 
            "height": 6.0,
            "max_weight": 1000,
            "levels": 3,
            "spacing": 0.15
          },
          "heavy_duty": {
            "width": 3.0,
            "depth": 1.2,
            "height": 8.0, 
            "max_weight": 2500,
            "levels": 4,
            "spacing": 0.2
          },
          "high_density": {
            "width": 2.5,
            "depth": 1.0,
            "height": 10.0,
            "max_weight": 1200,
            "levels": 5,
            "spacing": 0.1
          }
        },
        "default_type": "standard_pallet",
        "default_orientation": "vertical",
        "min_aisle_width": 3.0
      },
      "zones": {
        "types": {
          "receiving": {
            "min_area": 200,
            "min_width": 10.0,
            "placement": "entrance"
          },
          "shipping": {
            "min_area": 200,
            "min_width": 10.0,
            "placement": "exit"
          },
          "picking": {
            "min_area": 100,
            "min_width": 5.0,
            "placement": "central"
          },
          "storage": {
            "min_area": 500,
            "min_width": 20.0,
            "placement": "flexible"
          },
          "hazardous": {
            "min_area": 100,
            "min_width": 8.0,
            "placement": "isolated",
            "special_requirements": ["ventilation", "fire_protection"]
          }
        }
      },
      "aisles": {
        "types": {
          "main": {
            "width": 4.5,
            "direction": "horizontal"
          },
          "cross": {
            "width": 3.5,
            "direction": "vertical" 
          },
          "service": {
            "width": 2.0,
            "direction": "flexible"
          }
        },
        "default_type": "main",
        "min_count": {
          "main": 2,
          "cross": 3
        }
      },
      "doors": {
        "types": {
          "loading_dock": {
            "width": 4.0,
            "height": 4.5,
            "count": 5
          },
          "personnel": {
            "width": 1.2,
            "height": 2.1,
            "count": 4
          },
          "emergency": {
            "width": 2.0,
            "height": 2.1,
            "count": 6
          }
        }
      },
      "equipment": {
        "types": {
          "forklift": {
            "width": 1.2,
            "length": 2.5,
            "turning_radius": 2.0,
            "required_clearance": 0.5
          },
          "pallet_jack": {
            "width": 0.8,
            "length": 1.8,
            "turning_radius": 1.5,
            "required_clearance": 0.3
          },
          "reach_truck": {
            "width": 1.3,
            "length": 2.8,
            "turning_radius": 2.2,
            "required_clearance": 0.6
          }
        },
        "default_type": "forklift"
      }
    },
    "optimization": {
      "goals": {
        "space_utilization": 0.8,
        "picking_efficiency": 0.7,
        "travel_distance": 0.6,
        "throughput": 0.9
      },
      "constraints": {
        "fire_safety": true,
        "building_codes": true,
        "accessibility": true,
        "workflow": true
      },
      "algorithms": {
        "default": "genetic",
        "options": {
          "genetic": {
            "population_size": 100,
            "generations": 50,
            "mutation_rate": 0.1,
            "crossover_rate": 0.8
          },
          "simulated_annealing": {
            "initial_temperature": 100,
            "cooling_rate": 0.95,
            "iterations": 1000
          },
          "reinforcement_learning": {
            "learning_rate": 0.01,
            "discount_factor": 0.9,
            "exploration_rate": 0.1
          }
        }
      }
    },
    "cad": {
      "output_format": "dxf",
      "version": "R2018",
      "layers": {
        "walls": {
          "color": 7,
          "linetype": "CONTINUOUS",
          "lineweight": 0.5
        },
        "racks": {
          "color": 1,
          "linetype": "CONTINUOUS",
          "lineweight": 0.35
        },
        "aisles": {
          "color": 3,
          "linetype": "CONTINUOUS",
          "lineweight": 0.25
        },
        "zones": {
          "color": 5,
          "linetype": "DASHDOT",
          "lineweight": 0.15
        },
        "doors": {
          "color": 2,
          "linetype": "CONTINUOUS",
          "lineweight": 0.35
        },
        "dimensions": {
          "color": 4,
          "linetype": "CONTINUOUS",
          "lineweight": 0.2
        },
        "annotations": {
          "color": 6,
          "linetype": "CONTINUOUS",
          "lineweight": 0.18
        }
      },
      "styles": {
        "text": {
          "font": "Arial",
          "height": 0.25,
          "width_factor": 1.0
        },
        "dimension": {
          "arrow_size": 0.18,
          "extension_line_offset": 0.0625,
          "text_gap": 0.09
        }
      }
    },
    "ui": {
      "theme": "light",
      "auto_save": true,
      "auto_save_interval": 300,
      "units_display": "metric",
      "grid_snap": true,
      "display_options": {
        "show_grid": true,
        "show_dimensions": true,
        "show_labels": true,
        "show_zones": true
      }
    },
    "system": {
      "threads": 4,
      "memory_limit": 2048,
      "log_level": "info",
      "temp_directory": "./temp",
      "backup_directory": "./backups",
      "backup_frequency": "daily"
    }
  }